n\n.button-login-form {\n    position: absolute \n}\n\n.img-login-cloud-1{\n    position: absolute; \n    top: 60%; \n    left: 3%; \n    z-index: 1;\n}\n\n.img-login-main {\n    width: 45%; \n    top: 25%; \n    left: 525px; \n    position: absolute;\n}\n\n.img-login-cloud-2 {\n    position: absolute; \n    margin-top: 4%; \n    margin-left: 23%;\n}\n\n.main-logo {\n    position: absolute; \n    font-family: Fredoka One, cursive; \n    letter-spacing: 20px; \n    font-size: 52px; \n    top: 10%; \n    left: 740px; \n    color: whitesmoke;\n}\n\n.img-logo {\n    position: absolute; \n    width: 5%; \n    top: 10%; \n    left: 640px;\n    -webkit-animation:spin 4s linear infinite;\n    -moz-animation:spin 4s linear infinite;\n    animation:spin 4s linear infinite;\n}\n\n@-moz-keyframes spin { \n    100% { \n        -moz-transform: rotate(360deg); \n    } \n}\n\n@-webkit-keyframes spin { \n    100% { \n        -webkit-transform: rotate(360deg); \n    } \n}\n\n@keyframes spin { \n    100% { \n        -webkit-transform: rotate(360deg); \n        transform:rotate(360deg); \n    } \n}\n\n.img-on-logo {\n    width: 50px;\n    height: 40px;\n    margin-bottom: 10px;\n    margin-left: -10px;\n    margin-right: 10px;\n}\n\n.navbar-colapse {\n    font-family: Fredoka One, cursive;\n    letter-spacing: 10px;\n    font-size: 32px;\n    color: white;\n}\n\n.nav-text-logo {\n    width: 25px;\n}\n\n.intro-text {\n    font-family: Satisfy, cursive; \n    letter-spacing: 4px; \n    font-size: 24px; \n    color: white;\n}\n\n.nav-profile {\n    border-radius: 100%; \n    width: 24px; \n    height: 24px; \n    margin-right: 3%;\n}\n\n.nav-email {\n    font-family: Sriracha, cursive; \n    color: white;\n}\n\n.logout-colapse {\n    margin-top: 7%; \n    font-family: Sriracha, cursive;\n}\n\n.logout-body {\n    width: 200px;\n    background-color: #3b7dd8;\n}\n\n.full-profile {\n    border-radius: 100%;\n}\n\n.width-scroll {\n    display: flex;\n    justify-content: space-between;\n}\n.task-container {\n    font-family: Quicksand, sans-serif; \n    margin-top: 100px;\n    width: 250px;\n    height: 32px;\n}\n\n.task-title-bar {\n    background-color: #3b7dd8; \n    color: whitesmoke;\n}\n\n.task-icon {\n    width: 24px; \n    height: 24px;\n}\n\n.task-card-container {\n    width: 100%; \n    background-color: #d6eaff; \n    margin-left: -15px; \n    max-height: 60vh;\n}\n\n.task-card {\n    font-size: 14px; \n    min-height: 150px;\n}\n\n.task-body {\n    border-radius: 5px;\n    background-color: white;\n}\n\n.add-btn-container {\n    width: 100%; \n    background-color: #add6ff; \n    margin-left: -15px;\n}\n\n.add-modal-img {\n    width: 24px; \n    height: 24px;\n}\n\n.add-task-modal {\n    font-family: Quicksand, sans-serif;\n}\n\n.action-button {\n    width: 16px;\n    height: 16px;\n    margin-right: 10px;\n    margin-top: 5px;\n}\n\n.no-task {\n    display: flex;\n    justify-content: center;\n    margin-left: 5px;\n}\n\n.no-task-img {\n    width: 64px; \n    height: 64px;\n    opacity: 40%;\n}\n\n.no-task-text {\n    text-align: center;\n    opacity: 40%;\n}\n\n.collapse-description {\n    padding: 5px;\n    padding-left: 20px;\n    text-align: justify;\n}\n\na:hover {\n    text-decoration: none;\n}\n\n.flex-custom {\n    display: flex;\n    justify-content: space-between;\n    width: 100%;\n}\n\n.date-sizing {\n    width: 45%;\n}\n\n.add-form-styling {\n    border-radius: 20px; \n    background-color: #a2c4e7\n}\n\n.btn-add-edit {\n    display: flex;\n    justify-content: flex-end;\n}"},"lineCount":null}},"error":null,"hash":"672aceabae805a7101ee3ccea42706fa","cacheData":{"env":{}}}